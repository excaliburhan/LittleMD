{
  "extends": "airbnb",
  "env": {
    "commonjs": true,
    "node": true,
    "browser": true,
  },
  "ecmaFeatures": {
    "es6": true
  },
  "globals": {
    "DEBUG": true,
    "nw": true,
    "$": true,
    "hljs": true,
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-param-reassign": [0],
    "no-console": [0],
    "one-var": [0],
    "one-var-declaration-per-line": [0],
    "global-require": [0],
    "newline-per-chained-call": [0],
    "semi": [2, "never"],
    "no-unused-expressions": [2, {
      "allowShortCircuit": true
    }],
    "max-len": [2, 100, 2, {
      "ignoreUrls": true,
      "ignoreComments": true
    }],
    "no-restricted-syntax": [2,
      "DebuggerStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "react/require-extension": [0],
    "import/no-extraneous-dependencies": [0],
    "import/newline-after-import": [0],
    "import/no-unresolved": [2, {
      "ignore": [
        "dir-components"
      ]
    }]
  }
}
